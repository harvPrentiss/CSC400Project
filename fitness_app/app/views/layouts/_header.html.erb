<header>
  <div>
    <div>
      <%= link_to "Pulse Fitness", root_path, class:"title"%>
      <nav class="nav">
        <div class="nav-mobile"></div>
        <ul class="navList">
          <% if signed_in? %>
            <li><%= link_to "Users", users_path, class:"navItem" %></li>
            <li><%= link_to "Profile", current_user, class:"navItem" %></li>
            <li><%= link_to "Settings", edit_user_path(current_user), class:"navItem" %></li>
            <li><%= link_to "Sign out", signout_path, method: "delete", class:"navItem" %></li>
          <% else %>
            <li><%= link_to "Sign in", signin_path, class:"navItem" %></li>
          <% end %>
        </ul>
      </nav>
    </div>
  </div>
</header>


    