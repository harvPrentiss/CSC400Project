<ul class="soundTrackList">
	<h1 class="soundtrackHeader">My Top 5</h1>
	<% if @user.song1 != "" && @user.artist1 != "" && @user.song1? && @user.artist1? %>
		<li class="soundtrackItem">1. <%= @user.song1 %> - <%= @user.artist1 %></li>
	<% end %>
	<% if @user.song2 != "" && @user.artist2 != "" && @user.song2? && @user.artist2? %>
		<li class="soundtrackItem">2. <%= @user.song2 %> - <%= @user.artist2 %></li>
	<% end %>
	<% if @user.song3 != "" && @user.artist3 != "" && @user.song3? && @user.artist3? %>
		<li class="soundtrackItem">3. <%= @user.song3 %> - <%= @user.artist3 %></li>
	<% end %>
	<% if @user.song4 != "" && @user.artist4 != "" && @user.song4? && @user.artist4? %>
		<li class="soundtrackItem">4. <%= @user.song4 %> - <%= @user.artist4 %></li>
	<% end %>
	<% if @user.song5 != "" && @user.artist5 != "" && @user.song5? && @user.artist5? %>
		<li class="soundtrackItem">5. <%= @user.song5 %> - <%= @user.artist5 %></li>
	<% end %>
</ul>